{
	"name": "twitter-poll-server",
	"version": "1.0.0",
	"description": "Teste Dev 07-07-2018 - Server",
	"scripts": {
		"format": "prettier --write \"**/*.ts\"",
		"lint": "tslint src/**/*.ts",
		"start": "tsc && node dist/src/main.js",
		"start:dev": "nodemon"
	},
	"dependencies": {
		"@nestjs/common": "^5.0.1",
		"@nestjs/core": "^5.0.1",
		"class-transformer": "0.1.9",
		"class-transformer-validator": "^0.5.0",
		"class-validator": "0.8.5",
		"express-serve-static-core": "^0.1.1",
		"morgan": "^1.9.0",
		"reflect-metadata": "^0.1.12",
		"rxjs": "^6.1.0",
		"twit": "^2.2.11",
		"typescript": "^2.6.2",
		"winston": "^2.4.1"
	},
	"devDependencies": {
		"@types/express": "^4.0.39",
		"@types/morgan": "^1.7.35",
		"@types/node": "^9.3.0",
        "@types/winston": "^2.3.9",
        "@types/twit": "^2.2.15",
		"nodemon": "^1.14.1",
		"prettier": "^1.11.1",
		"ts-node": "^4.1.0",
		"tsconfig-paths": "^3.1.1",
		"tslint": "^5.9.1"
	},
	"engines": {
		"node": "8.11.1",
		"npm": "5.6.0",
		"yarn": "1.7.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "../coverage"
	}
}
